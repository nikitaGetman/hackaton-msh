A,IAAA,CAFA,CADA;AAKA,UAAA,iBAAA,EAAA;AALA;AAHA,OADA;AAYA,MAAA,GAAA,EAAA,IAZA;AAaA,MAAA,aAAA,EAAA,IAbA;AAcA,MAAA,KAAA,EAAA;AAAA,QAAA,QAAA,EAAA,IAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAdA;AAeA,MAAA,aAAA,EAAA,IAfA;AAgBA,MAAA,WAAA,EAAA,KAhBA;AAkBA,MAAA,cAAA,EAAA,KAlBA;AAmBA,MAAA,YAAA,EAAA;AAnBA,KAAA;AAqBA,GA1BA;AA2BA,EAAA,QAAA,EAAA,EA3BA;AA4BA,EAAA,OA5BA,qBA4BA,CAAA,CA5BA;AA6BA,EAAA,OAAA,EAAA;AACA,IAAA,oBADA,gCACA,CADA,EACA;AAAA;;AACA,UAAA,QAAA,GAAA,CAAA,CAAA,GAAA,CAAA,UAAA,CAAA;AACA,UAAA,IAAA,GAAA,KAAA,aAAA,CAAA,OAAA,CAAA,OAAA,CAAA,QAAA,CAAA;AACA,UAAA,cAAA,GAAA,IAAA,CAAA,QAAA,CAAA,IAAA;;AACA,UAAA,cAAA,KAAA,WAAA,EAAA;AACA,YAAA,KAAA,KAAA,CAAA,QAAA,EAAA;AACA,eAAA,aAAA,CAAA,OAAA,CAAA,gBAAA,CAAA,KAAA,KAAA,CAAA,QAAA,EAAA;AACA,YAAA,WAAA,EAAA;AADA,WAAA;AAGA;;AACA,aAAA,aAAA,CAAA,OAAA,CAAA,gBAAA,CAAA,QAAA,EAAA;AACA,UAAA,WAAA,EAAA;AADA,SAAA;AAGA,aAAA,KAAA,CAAA,QAAA,GAAA,QAAA;AACA,aAAA,KAAA,CAAA,IAAA,GAAA,IAAA;AACA,aAAA,WAAA,GAAA,IAAA;AACA,aAAA,YAAA,GAAA,IAAA;AACA,QAAA,UAAA,CAAA,YAAA;AACA,UAAA,KAAA,CAAA,YAAA,GAAA,KAAA;AACA,SAFA,EAEA,IAFA,CAAA;AAGA;AACA,KAtBA;AAuBA,IAAA,MAvBA,kBAuBA,GAvBA,EAuBA;AACA,WAAA,GAAA,GAAA,GAAA;AACA,WAAA,aAAA,GAAA,IAAA,KAAA,CAAA,aAAA,EAAA;AACA,WAAA,aAAA,CAAA,OAAA,CAAA,MAAA,CAAA,GAAA,CAAA,CAAA,OAAA,CAAA,EAAA,KAAA,oBAAA,EAHA,CAKA;AACA,KA7BA;AA8BA,IAAA,MA9BA,oBA8BA;AAAA;;AACA,WAAA,cAAA,GAAA,IAAA;AACA,MAAA,UAAA,CAAA,YAAA;AACA,YAAA,OAAA,GAAA,eAAA,CAAA;AACA,UAAA,WAAA,EAAA,CAAA,KAAA,EAAA,KAAA,CADA;AAEA,UAAA,UAAA,EAAA,EAFA;AAGA,UAAA,UAAA,EAAA;AAHA,SAAA,CAAA;AAKA,YAAA,KAAA,GAAA,kBAAA,CAAA,OAAA,CAAA;AAEA,YAAA,OAAA,GAAA,eAAA,CAAA,KAAA,CAAA;;AAEA,QAAA,MAAA,CAAA,aAAA,CAAA,SAAA;;AACA,QAAA,MAAA,CAAA,aAAA,CAAA,GAAA,CAAA,OAAA;;AAEA,QAAA,MAAA,CAAA,GAAA,CAAA,UAAA,CAAA,GAAA,CAAA,MAAA,CAAA,aAAA;;AAEA,QAAA,MAAA,CAAA,cAAA,GAAA,KAAA;AACA,OAhBA,EAgBA,GAhBA,CAAA;AAiBA,KAjDA;AAmDA,IAAA,UAnDA,wBAmDA;AACA,WAAA,WAAA,GAAA,KAAA;AACA;AArDA;AA7BA,CAAA","sourcesContent":["<template>\n  <v-app>\n    <v-app-bar app color=\"primary\" dark>\n      <v-app-bar-nav-icon @click=\"isSidebarOpen = !isSidebarOpen\" />\n    </v-app-bar>\n\n    <v-main>\n      <v-container fluid class=\"dashboard fill-height align-stretch\">\n        <v-row>\n          <!-- <transition name=\"sidebar-slide\"> -->\n          <v-col v-if=\"isSidebarOpen\" :cols=\"3\" class=\"dashboard__sidebar\">\n            <the-sidebar :loading=\"sidebarLoading\" @submit=\"search\" />\n          </v-col>\n          <!-- </transition> -->\n\n          <v-col :cols=\"isSidebarOpen ? 9 : 12\" class=\"dashboard__map-wrapper pa-0\">\n            <yandex-map\n              :options=\"mapConfig.options\"\n              :settings=\"$options.MAP_SETTINGS\"\n              :coords=\"mapConfig.coords\"\n              :controls=\"[]\"\n              :zoom=\"mapConfig.zoom\"\n              @map-was-initialized=\"setMap\"\n            >\n            </yandex-map>\n          </v-col>\n        </v-row>\n      </v-container>\n\n      <v-bottom-sheet :value=\"isSheetOpen\" inset hide-overlay persistent no-click-animation>\n        <v-sheet class=\"sheet pt-4 px-2\">\n          <base-loader :active=\"sheetLoading\" />\n          <v-btn icon class=\"sheet__close\" @click=\"closeSheet\">\n            <v-icon>mdi-close</v-icon>\n          </v-btn>\n          <div class=\"sheet__content\">\n            <div class=\"mt-1 mx-3\">\n              Данные и рекомендации для участка\n            </div>\n          </div>\n        </v-sheet>\n      </v-bottom-sheet>\n    </v-main>\n  </v-app>\n</template>\n\n<script>\n/* eslint-disable no-undef */\nimport { yandexMap } from 'vue-yandex-maps'\nimport TheSidebar from '@/components/TheSidebar.vue'\nimport BaseLoader from '@/components/BaseLoader.vue'\nimport { generateSquares, generateRectangles, generateObjects } from '@/utils/generator'\n\nconst DEFAULT_MOSCOW_COORDS = Object.freeze([55.7503535552648, 37.61581057094254])\nconst MAP_SETTINGS = {\n  apiKey: process.env.VUE_APP_MAP_KEY,\n  lang: 'ru_RU',\n  coordorder: 'latlong',\n  version: '2.1'\n}\n\nexport default {\n  name: 'Dashboard',\n  components: { yandexMap, TheSidebar, BaseLoader },\n  MAP_SETTINGS,\n  data() {\n    return {\n      mapConfig: {\n        coords: DEFAULT_MOSCOW_COORDS,\n        zoom: 12,\n        options: {\n          restrictMapArea: [\n            [55.4, 37],\n            [56, 38.2]\n          ],\n          autoFitToViewport: 'always'\n        }\n      },\n      map: null,\n      objectManager: null,\n      model: { objectId: null, data: null },\n      isSidebarOpen: true,\n      isSheetOpen: false,\n\n      sidebarLoading: false,\n      sheetLoading: false\n    }\n  },\n  computed: {},\n  created() {},\n  methods: {\n    handleRectangleClick(e) {\n      const objectId = e.get('objectId')\n      const zone = this.objectManager.objects.getById(objectId)\n      const objectGeometry = zone.geometry.type\n      if (objectGeometry === 'Rectangle') {\n        if (this.model.objectId) {\n          this.objectManager.objects.setObjectOptions(this.model.objectId, {\n            strokeWidth: 0\n          })\n        }\n        this.objectManager.objects.setObjectOptions(objectId, {\n          strokeWidth: 2\n        })\n        this.model.objectId = objectId\n        this.model.data = zone\n        this.isSheetOpen = true\n        this.sheetLoading = true\n        setTimeout(() => {\n          this.sheetLoading = false\n        }, 1000)\n      }\n    },\n    setMap(map) {\n      this.map = map\n      this.objectManager = new ymaps.ObjectManager()\n      this.objectManager.objects.events.add(['click'], this.handleRectangleClick)\n\n      // this.search()\n    },\n    search() {\n      this.sidebarLoading = true\n      setTimeout(() => {\n        const squares = generateSquares({\n          startCoords: [55.79, 37.54],\n          matrixSize: 20,\n          sideLength: 0.007\n        })\n        const rects = generateRectangles(squares)\n\n        const objects = generateObjects(rects)\n\n        this.objectManager.removeAll()\n        this.objectManager.add(objects)\n\n        this.map.geoObjects.add(this.objectManager)\n\n        this.sidebarLoading = false\n      }, 100)\n    },\n\n    closeSheet() {\n      this.isSheetOpen = false\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\">\n.dashboard {\n  &__map-wrapper {\n    // transition: all 0.3s;\n    .ymap-container {\n      width: 100%;\n      height: 100%;\n    }\n  }\n\n  // .sidebar-slide-enter-active {\n  //   transition: all 0.3s;\n  // }\n  // .sidebar-slide-leave-active {\n  //   transition: all 0.3s;\n  // }\n  // .sidebar-slide-enter,\n  // .sidebar-slide-leave-to {\n  //   transform: translateX(100px);\n  //   opacity: 0;\n  // }\n}\n\n.sheet {\n  display: flex;\n  flex-direction: column;\n  position: relative;\n  min-height: 200px;\n  max-height: 250px;\n\n  &__close {\n    position: absolute;\n    right: 12px;\n    top: 12px;\n  }\n  &__content {\n    overflow-y: auto;\n  }\n}\n</style>\n"],"sourceRoot":"src/views"}]}